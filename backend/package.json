{
  "name": "venture-launch-backend",
  "version": "1.0.0",
  "description": "Venture Launch application backend",
  "scripts": {
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "dev": "ts-node ./src/server.ts",
    "prod": "node ./dist/server.js",
    "dev:nodemon": "./node_modules/nodemon/bin/nodemon.js",
    "build": "npx tsc",
    "database:migration:create": "npm run typeorm -- migration:create src/typeorm/migrations/${npm_config_name}",
    "database:migration:generate": "npm run typeorm -- migration:generate src/typeorm/migrations/${npm_config_name} -d src/typeorm/index.typeorm.ts",
    "database:migration:run": "npm run typeorm -- migration:run -d src/typeorm/index.typeorm.ts",
    "database:migration:revert": "npm run typeorm -- migration:revert -d src/typeorm/index.typeorm.ts",
    "database:drop": "npm run typeorm -- schema:drop -d src/typeorm/index.typeorm.ts"
  },
  "author": "Venture Launch",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/lodash": "^4.17.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.25",
    "@types/qs": "^6.9.15",
    "@types/uuid": "^9.0.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@pinata/sdk": "^2.1.0",
    "@thirdweb-dev/storage": "^2.0.11",
    "@types/bcrypt": "^5.0.2",
    "amqplib": "^0.10.4",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bs58": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "googleapis": "^140.0.0",
    "jose": "^5.4.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "pg": "^8.11.3",
    "qs": "^6.12.1",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.7.5",
    "tweetnacl": "^1.0.3",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  }
}
